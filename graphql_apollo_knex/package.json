{
  "name": "graphql_apollo",
  "version": "1.0.0",
  "description": "GraphQL Apollo Server",
  "main": "./src/main.ts",
  "author": "ralvescosta",
  "scripts": {
    "start:dev": "NODE_ENV=development yarn nodemon --inspect=0.0.0.0:9333 src/main.ts",
    "build": "tsc",
    "migration:run": "yarn knex --knexfile ./src/core/infra/database/knexfile.ts migration:latest",
    "migration:create": "yarn knex --knexfile ./src/core/infra/database/knexfile.ts -x ts migration:make",
    "migration:undo": "yarn knex --knexfile ./src/core/infra/database/knexfile.ts migration:rollback"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "apollo-server": "^2.16.1",
    "graphql": "^15.3.0",
    "knex": "^0.21.5",
    "sqlite3": "^5.0.0",
    "tsc": "^1.20150623.0"
  }
}
